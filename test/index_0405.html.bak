<script>
    $('.media-controls').on('click', function(){
        var vid = document.getElementById("headvideo");
        if($(this).hasClass('active')){
            vid.muted = true;
            $(this).removeClass('active');
        }
        else{
            vid.muted = false;
            $(this).addClass('active');
        }
    });
    $('#steps_1_c').on('click', function(){
        $('#steps_2_c').removeClass('active');
        $('#steps_3_c').removeClass('active');
        $('#steps_2_c').next('p').text('');
        $('#steps_3_c').next('p').text('');
        $(this).addClass('active');
        $(this).next('p').text('1000s of hours of great TV shows, telenovelas, comedies, travel, and more. New shows added monthly.');
        $('#mac_iphone2').fadeOut();
        $('#mac_iphone3').fadeOut();
        setTimeout(() => {
            $('#mac_iphone').fadeIn();
        }, 400);
    });
    $('#steps_2_c').on('click', function(){
        $('#steps_1_c').removeClass('active');
        $('#steps_3_c').removeClass('active');
        $('#steps_1_c').next('p').text('');
        $('#steps_3_c').next('p').text('');
        $(this).addClass('active');
        $(this).next('p').text('All shows come with 2 sets of subtitles. Click any word to get an instant translation. The more you watch, the more you learn.');
        $('#mac_iphone').fadeOut();
        $('#mac_iphone3').fadeOut();
        setTimeout(() => {
            $('#mac_iphone2').fadeIn();
        }, 400);
    });
    $('#steps_3_c').on('click', function(){
        $('#steps_1_c').removeClass('active');
        $('#steps_2_c').removeClass('active');
        $('#steps_1_c').next('p').text('');
        $('#steps_2_c').next('p').text('');
        $(this).addClass('active');
        $(this).next('p').text('Review what you learned with contextual & video based flashcards. Track your progress as you watch and see your confidence grow.');
        $('#mac_iphone').fadeOut();
        $('#mac_iphone2').fadeOut();
        setTimeout(() => {
            $('#mac_iphone3').fadeIn();
        }, 400);
    });

</script>
